{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "jasmine": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "constructor-super":1,
        "curly":1,
        "default-case":1,
        "eqeqeq":1,
        "indent":[1, 2, { "SwitchCase": 1 }],
        "jsx-quotes":1,
        "max-len":[2, { "code": 240 }],
        "max-lines":[1, 1000],
        "no-alert":1,
        "no-case-declarations":1,
        "no-class-assign":1,
        "no-compare-neg-zero":1,
        "no-cond-assign":1,
        "no-confusing-arrow":1,
        "no-console":1,
        "no-const-assign":1,
        "no-constant-condition":1,
        "no-control-regex":1,
        "no-debugger":1,
        "no-div-regex":1,
        "no-dupe-args":1,
        "no-dupe-class-members":1,
        "no-dupe-keys":1,
        "no-duplicate-case":1,
        "no-empty":1,
        "no-empty-character-class":1,
        "no-empty-pattern":1,
        "no-ex-assign":1,
        "no-extra-boolean-cast":1,
        "no-extra-semi":1,
        "no-fallthrough":1,
        "no-func-assign":1,
        "no-global-assign":1,
        "no-inner-declarations":1,
        "no-invalid-regexp":1,
        "no-irregular-whitespace":1,
        "no-labels":1,
        "no-mixed-spaces-and-tabs":1,
        "no-new-symbol":1,
        "no-obj-calls":1,
        "no-octal":1,
        "no-redeclare":1,
        "no-regex-spaces":1,
        "no-restricted-properties":1,
        "no-sparse-arrays":1,
        "no-this-before-super":1,
        "no-undef":1,
        "no-unexpected-multiline":1,
        "no-unreachable":1,
        "no-unsafe-finally":1,
        "no-unsafe-negation":1,
        "no-unused-labels":1,
        "no-unused-vars":1,
        "no-use-before-define":1,
        "no-useless-escape":1,
        "no-var":1,
        "quotes": [2, "single"],
        "require-yield":1,
        "semi":1,
        "use-isnan":1,
        "valid-typeof":1
    }
}
