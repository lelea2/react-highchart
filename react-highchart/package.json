{
  "name": "@prosoft/realsuite-ui-react-highchart",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "ProSoft Engineering",
  "license": "ISC",
  "private": false,
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "start": "NODE_ENV=development webpack-dev-server",
    "lint": "eslint src/**/*.js",
    "test": "jest --coverage --forceExit",
    "watch": "NODE_ENV=development webpack-dev-server --watch"
  },
  "dependencies": {
    "@prosoft/realsuite-common-client": "^4.8.1-alpha.f061d332",
    "highcharts": "^6.1.1",
    "highcharts-react-official": "^1.3.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^22.4.2",
    "jest-enzyme": "^4.2.0",
    "react-test-renderer": "^16.3.0",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setupTests.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!/tests/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/tests/fileMock.js"
    }
  }
}
